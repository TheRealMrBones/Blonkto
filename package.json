{
    "name": "blonkto",
    "version": "0.1.0",
    "description": "GET BLONKED! multiplayer game to host servers for your friends",
    "private": true,
    "main": "src/server/server.js",
    "type": "module",
    "scripts": {
        "prod:pack": "webpack --config webpack.prod.js",
        "prod:build": "tsc -p tsconfig.nodejs.json",
        "prod": "npm run prod:pack && npm run prod:build",
        "dev:pack": "webpack --config webpack.dev.js",
        "dev:start": "nodemon src/server/server.js",
        "dev": "npm run dev:pack && npm run dev:start",
        "lint": "eslint \"src/**/*.{js,mjs,cjs,ts}\"",
        "format": "eslint \"src/**/*.{js,mjs,cjs,ts}\" --fix",
        "resetworld": "rd /s /q data\\world & rd /s /q data\\entities & rd /s /q data\\players & rm -rf data\\world & rm -rf data\\entities & rm -rf data\\players"
    },
    "keywords": [],
    "author": "Mr Bones",
    "devDependencies": {
        "@babel/core": "^7.26.9",
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-typescript": "^7.26.0",
        "@eslint/js": "^9.20.0",
        "@types/express": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/node": "^22.13.4",
        "@types/throttle-debounce": "^5.0.2",
        "@types/validator": "^13.12.2",
        "babel-loader": "^9.2.1",
        "css-loader": "^7.1.2",
        "eslint": "^9.20.1",
        "eslint-plugin-import": "^2.31.0",
        "globals": "^15.15.0",
        "jsdoc": "^4.0.4",
        "mini-css-extract-plugin": "^2.9.2",
        "nodemon": "^3.1.9",
        "ts-loader": "^9.5.2",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.24.0",
        "webpack": "^5.98.0",
        "webpack-cli": "^5.1.4",
        "webpack-merge": "^6.0.1"
    },
    "dependencies": {
        "argon2": "^0.41.1",
        "events": "^3.3.0",
        "express": "^4.21.2",
        "html-webpack-plugin": "^5.6.3",
        "jsonwebtoken": "^9.0.2",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "throttle-debounce": "^5.0.2",
        "validator": "^13.12.0",
        "webpack-dev-middleware": "^7.4.2"
    }
}
